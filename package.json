{
  "name": "m.find",
  "version": "0.0.1",
  "description": "m(icro)find is a lightweight find utility written in es6+",
  "main": "index.js",
  "man": [
    "man/m.find.1"
  ],
  "bin": {
    "m.find": "bin/m.find"
  },
  "standard": {
    "globals": [
      "test"
    ]
  },
  "scripts": {
    "node": "node",
    "start": "bin/m.find",
    "pretest": "standard",
    "test": "m.test",
    "coverage": "node --require ./node_modules/m.test/global.js ./node_modules/istanbul/lib/cli.js cover --print detail test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoputzer/m.find.git"
  },
  "keywords": [
    "files",
    "find",
    "look-up",
    "file",
    "search",
    "match",
    "package",
    "resolve",
    "folder",
    "directory",
    "dir",
    "walk",
    "walking",
    "path",
    "m.icro"
  ],
  "license": "MIT",
  "author": "Ivo von Putzer Reibegg <ivo.putzer@gmail.com>",
  "bugs": {
    "url": "https://github.com/ivoputzer/m.find/issues"
  },
  "homepage": "https://github.com/ivoputzer/m.find#readme",
  "dependencies": {
    "find": "^0.2.9",
    "m.flatten": "^1.0.1"
  },
  "devDependencies": {
    "standard": "^11.0.1",
    "m.test": "0.0.57"
  },
  "optionalDependencies": {
    "istanbul": "^0.4.5"
  },
  "directories": {
    "test": "test"
  }
}
